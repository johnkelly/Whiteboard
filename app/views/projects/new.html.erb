<div class="page-header center">
  <h1>New Project</h1>
</div>
<div class="well span-7 offset4">
  <%= form_for(@project) do |f| %>
    <div class="input-prepend">
      <%= f.label :name %>
      <span class="add-on"><i class="icon-font"></i></span><%= f.text_field :name %>
    </div>

    <div class="center mt15"><%= f.submit "Create", class: "btn btn-success center", data: { disable_with: "Creating..."} %></div>
  <% end %>
</div>